#include <bits/stdc++.h>
using namespace std;

int main()
{
 int n;
 cin>>n;
 int arr[n];
 for(int i=0;i<n;i++){
     cin>>arr[i];
 }
 int m=*max_element(arr,arr+n);
 if(m<0){
     cout<<m;
     return 0;
 }
 int res=0;
 for(int i=0;i<n;i++){
     int product=arr[i];
     for(int j=i+1;j<n;j++){
         product=product+arr[j];
         res=max(product,res);
     }
 }
std::cout << res << std::endl;
}

